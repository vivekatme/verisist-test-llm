  "clinicalInterpretation": {
    "acute_primary_dengue": {
      "condition": "NS1 positive AND IgM positive AND IgG negative",
      "interpretation": "Acute primary dengue infection"
    },
    "acute_secondary_dengue": {
      "condition": "NS1 positive AND IgM positive AND IgG positive",
      "interpretation": "Acute secondary dengue infection (higher risk of severe dengue)"
    },
    "recent_infection": {
      "condition": "NS1 negative AND IgM positive AND IgG negative/positive",
      "interpretation": "Recent dengue infection (after Day 7)"
    },
    "past_infection": {
      "condition": "NS1 negative AND IgM negative AND IgG positive",
      "interpretation": "Past dengue infection (immunity present)"
    },
    "no_infection": {
      "condition": "NS1 negative AND IgM negative AND IgG negative",
      "interpretation": "No current or past dengue infection detected"
    }
  },

